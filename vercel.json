{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/email/flask_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/email/(.*)",
      "dest": "/api/email/flask_app.py"
    },
    {
      "src": "/logo.png",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
